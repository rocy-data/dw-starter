<?xml version="1.1" encoding="UTF-8"?>
<xs:schema
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:index_category="https://www.rocy-data.com/dw/index_category"
        xmlns:market="https://www.rocy-data.com/dw/market"
        xmlns:owner="https://www.rocy-data.com/dw/owner"
        xmlns:standard_index="https://www.rocy-data.com/dw/standard_index"
        xmlns:standard_fields="https://www.rocy-data.com/dw/standard_fields"
        xmlns:subject="https://www.rocy-data.com/dw/subject"
        xmlns:refs="https://www.rocy-data.com/dw/refs"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
        https://www.rocy-data.com/dw/index_category ../dw/schema/index_category.xsd
        https://www.rocy-data.com/dw/market ../dw/schema/market.xsd
        https://www.rocy-data.com/dw/owner ../dw/schema/owner.xsd
        https://www.rocy-data.com/dw/standard_index ../dw/schema/standard_index.xsd
        https://www.rocy-data.com/dw/standard_fields ./standard_fields.xsd
        https://www.rocy-data.com/dw/subject ../dw/schema/subject.xsd
        https://www.rocy-data.com/dw/refs ./dw_refs.xsd"
        targetNamespace="https://www.rocy-data.com/dw/v1.0.0"
        elementFormDefault="qualified"
>
    <xs:import schemaLocation="../dw/schema/index_category.xsd" namespace="https://www.rocy-data.com/dw/index_category"/>
    <xs:import schemaLocation="../dw/schema/market.xsd" namespace="https://www.rocy-data.com/dw/market"/>
    <xs:import schemaLocation="../dw/schema/owner.xsd" namespace="https://www.rocy-data.com/dw/owner"/>
    <xs:import schemaLocation="../dw/schema/standard_index.xsd" namespace="https://www.rocy-data.com/dw/standard_index"/>
    <xs:import schemaLocation="./standard_fields.xsd" namespace="https://www.rocy-data.com/dw/standard_fields"/>
    <xs:import schemaLocation="../dw/schema/subject.xsd" namespace="https://www.rocy-data.com/dw/subject"/>
    <xs:import schemaLocation="dw_refs.xsd" namespace="https://www.rocy-data.com/dw/refs"/>

    <xs:element name="compose" type="compose"/>
    <xs:element name="dw" type="dw"/>
    <xs:element name="dw_pipeline" type="dw_pipeline"/>

    <xs:complexType name="compose">
        <xs:all>
            <xs:element type="path_list" name="path_list" minOccurs="0"/>
            <xs:element type="constants" name="constants" minOccurs="0"/>
            <xs:element name="modules">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="module" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="path" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="pipelines">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="pipeline" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="path" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:all>
        <xs:attribute name="active_profile" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="constants">
        <xs:sequence>
            <xs:element name="constant" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:attribute name="name" type="xs:string" use="required"/>
                    <xs:attribute name="value" type="xs:string" use="required"/>
                    <xs:attribute name="description" type="xs:string"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="path_list">
        <xs:sequence>
            <xs:element name="profile" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="path" type="path" maxOccurs="unbounded"/>
                    </xs:sequence>
                    <xs:attribute name="name" use="required"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="path">
        <xs:attribute name="overwrite" type="xs:boolean" default="false"/>
        <xs:attribute name="name" type="xs:Name" use="required"/>
        <xs:attribute name="path" type="xs:anyURI" use="required"/>
        <xs:attribute name="parent">
            <xs:simpleType>
                <xs:union memberTypes="refs:valid_path">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="RX_DATA_DW_HOME"/>
                            <xs:enumeration value="PROJECT_PATH"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:union>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="pattern" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="none"/>
                    <xs:enumeration value="day"/>
                    <xs:enumeration value="week"/>
                    <xs:enumeration value="month"/>
                    <xs:enumeration value="quarter"/>
                    <xs:enumeration value="half_year"/>
                    <xs:enumeration value="this_year"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="partition_count" type="xs:int" default="10"/>
        <xs:attribute name="description" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="dw_pipeline">
        <xs:sequence>
            <xs:element type="path_list" name="path_list" minOccurs="0"/>
            <xs:element type="constants" name="constants" minOccurs="0"/>
            <xs:element type="computed_column_groups" name="computed_column_groups" minOccurs="0"/>
            <xs:element type="common_computed_columns" name="common_computed_columns" minOccurs="0"/>
            <xs:element type="columns_group" name="columns_group" minOccurs="0"/>
            <xs:element type="transforms_pipeline" name="transforms"/>
        </xs:sequence>
        <xs:attribute name="dw_market" type="market:market" use="required"/>
        <xs:attribute name="dw_subject" type="subject:subject" use="required"/>
        <xs:attribute name="owner" type="owner:owner" use="required"/>
        <xs:attribute name="dw_level" type="dw_level" use="required"/>
    </xs:complexType>
    <xs:complexType name="dw">
        <xs:annotation>
            <xs:documentation>
                <![CDATA[
                          SQL ORDERS:

                          1-name_standardize_list
                          2-value_standardize_list
                          3-boolean_convert_list
                          4-distinct_by
                          5-flat_json
                          6-where
                          7-computed_columns_ref/computed_column_group <===
                          8-where_after_compute
                          9-group_where_list/groups/measures
                          10-having
                          11-select
                          12-drop
                          13-order_by
                    ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element type="path_list" name="path_list" minOccurs="0"/>
            <xs:element type="constants" name="constants" minOccurs="0"/>
            <xs:element type="computed_column_groups" name="computed_column_groups" minOccurs="0"/>
            <xs:element type="common_computed_columns" name="common_computed_columns" minOccurs="0"/>
            <xs:element type="columns_group" name="columns_group" minOccurs="0"/>
            <xs:element type="sources" name="sources" minOccurs="0"/>
            <xs:element type="transforms" name="transforms" minOccurs="0"/>
            <xs:element type="sinks" name="sinks" minOccurs="0"/>
        </xs:all>
        <xs:attribute name="dw_market" type="market:market" use="required"/>
        <xs:attribute name="dw_subject" type="subject:subject" use="required"/>
        <xs:attribute name="owner" type="owner:owner" use="required"/>
        <xs:attribute name="dw_level" type="dw_level" use="required"/>
    </xs:complexType>

    <xs:complexType name="sources">
        <xs:sequence>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element name="source_dictionary" type="source_dictionary"/>
                <xs:element name="source_csv" type="source_csv"/>
                <xs:element name="source_text" type="source_text"/>
                <xs:element name="source_json" type="source_json"/>
                <xs:element name="source_parquet" type="source_parquet"/>
                <xs:element name="source_protocol_bytes" type="source_protocol_bytes"/>
                <xs:element name="source_sequence_file" type="source_sequence_file"/>
                <xs:element name="source_sequence_constants" type="source_sequence_constants"/>
                <xs:element name="source_jdbc" type="source_jdbc"/>
                <xs:element name="source_plugin" type="source_plugin"/>
                <xs:element name="source_xml" type="source_xml"/>
                <xs:element name="source_whole_text_files" type="source_path"/>
                <xs:element name="source_hbase" type="source_hbase"/>
                <xs:element name="source_elastic_search" type="source_elastic_search"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="transforms">
        <xs:sequence>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element name="transform_schema" type="transform_one_ref"/>
                <xs:element name="transform_plugin" type="transform_plugin"/>
                <xs:element name="transform_transform" type="transform_one_ref"/>
                <xs:element name="transform_flat_json" type="transform_flat_json"/>
                <xs:element name="transform_summary" type="transform_one_ref"/>
                <xs:element name="transform_summary_text" type="transform_one_ref"/>
                <xs:element name="transform_split" type="transform_split"/>
                <xs:element name="transform_data_governance" type="transform_data_governance"/>
                <xs:element name="transform_pipeline">
                    <xs:complexType>
                        <xs:complexContent>
                            <xs:extension base="transform_one_ref">
                                <xs:attribute name="pipeline_ref" use="required" type="refs:pipeline_id"/>
                            </xs:extension>
                        </xs:complexContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="transform_quality" type="transform_quality"/>
                <xs:element name="transform_dimension">
                    <xs:complexType>
                        <xs:complexContent>
                            <xs:extension base="transform_one_ref">
                                <xs:attribute name="dimensions" type="xs:string"/>
                            </xs:extension>
                        </xs:complexContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="transform_join" type="transform_join"/>
                <xs:element name="transform_dimension_merge" type="transform_dimension_merge"/>
                <xs:element name="transform_join_dimensions" type="transform_join_dimensions"/>
                <xs:element name="transform_union" type="transform_union"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="transforms_pipeline">
        <xs:sequence>
            <xs:choice>
                <xs:element name="transform_schema" type="pipeline_action"/>
                <xs:element name="transform_plugin">
                    <xs:complexType>
                        <xs:complexContent>
                            <xs:extension base="transform_plugin">
                                <xs:attributeGroup ref="pipeline_no_ref"/>
                            </xs:extension>
                        </xs:complexContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="transform_transform" type="pipeline_action"/>
                <xs:element name="transform_flat_json">
                    <xs:complexType>
                        <xs:complexContent>
                            <xs:extension base="transform_flat_json">
                                <xs:attributeGroup ref="pipeline_no_ref"/>
                            </xs:extension>
                        </xs:complexContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="transform_summary" type="pipeline_action"/>
                <xs:element name="transform_summary_text" type="pipeline_action"/>
                <xs:element name="transform_split">
                    <xs:complexType>
                        <xs:complexContent>
                            <xs:extension base="transform_split">
                                <xs:attributeGroup ref="pipeline_no_ref"/>
                            </xs:extension>
                        </xs:complexContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="transform_data_governance">
                    <xs:complexType>
                        <xs:complexContent>
                            <xs:extension base="transform_data_governance">
                                <xs:attributeGroup ref="pipeline_no_ref"/>
                            </xs:extension>
                        </xs:complexContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="transform_pipeline">
                    <xs:complexType>
                        <xs:complexContent>
                            <xs:extension base="pipeline_action">
                                <xs:attribute name="pipeline_ref" use="required" type="refs:pipeline_id"/>
                            </xs:extension>
                        </xs:complexContent>
                    </xs:complexType>
                </xs:element>
                <xs:element name="transform_quality">
                    <xs:complexType>
                        <xs:complexContent>
                            <xs:extension base="transform_quality">
                                <xs:attributeGroup ref="pipeline_no_ref"/>
                            </xs:extension>
                        </xs:complexContent>
                    </xs:complexType>
                </xs:element>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="sinks">
        <xs:sequence>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element name="sink_csv" type="sink_csv"/>
                <xs:element name="sink_json" type="sink_json"/>
                <xs:element name="sink_parquet" type="sink_parquet"/>
                <xs:element name="sink_sequence_file" type="sink_sequence_file"/>
                <xs:element name="sink_jdbc" type="sink_jdbc"/>
                <xs:element name="sink_plugin" type="sink_plugin"/>
                <xs:element name="sink_xml" type="sink_xml"/>
                <xs:element name="sink_hbase" type="sink_hbase"/>
                <xs:element name="sink_elastic_search" type="sink_elastic_search"/>
                <xs:element name="sink_show" type="sink_show"/>
                <xs:element name="sink_join_fill" type="sink_join_fill"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="action">
        <xs:all>
            <xs:element name="measures" minOccurs="0" type="measures"/>
            <xs:element name="computed_column_group" minOccurs="0" type="computed_column_group"/>
            <xs:element name="group_where_list" minOccurs="0" type="group_where_list"/>
            <xs:element name="boolean_convert_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="boolean_convert" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="names_regex" type="xs:string"/>
                                <xs:attribute name="full_names" type="xs:string"/>
                                <xs:attribute name="start_with" type="xs:string"/>
                                <xs:attribute name="end_with" type="xs:string"/>
                                <xs:attribute name="true_values" type="xs:string" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="name_standardize_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="name_standardize">
                            <xs:complexType>
                                <xs:attribute name="name" type="xs:Name" use="required"/>
                                <xs:attribute name="standardized_name" type="standard_fields:standard_fields"
                                              use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="value_standardize_list" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="value_standardize">
                            <xs:annotation>
                                <xs:documentation>
                                    <![CDATA[
                                           expression ex: 0-250_-40-210
                                           it means that convert the value within range [0,250] to
                                    [40-210]
                                    ]]>
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:attribute name="name" type="xs:string" use="required"/>
                                <xs:attribute name="expression" type="xs:string" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="schema_definitions" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="field" minOccurs="0">
                            <xs:complexType>
                                <xs:attribute name="field_name" type="xs:Name"/>
                                <xs:attribute name="data_type">
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                            <xs:enumeration value="STRING"/>
                                            <xs:enumeration value="BOOLEAN"/>
                                            <xs:enumeration value="INT"/>
                                            <xs:enumeration value="FLOAT"/>
                                            <xs:enumeration value="DOUBLE"/>
                                            <xs:enumeration value="LONG"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="content" type="xs:string"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="selects" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="select">
                            <xs:complexType>
                                <xs:attribute name="select_pattern">
                                    <xs:annotation>
                                        <xs:documentation>regex pattern to match the field name</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string"/>
                                    </xs:simpleType>
                                </xs:attribute>
                                <xs:attribute name="drop_pattern">
                                    <xs:annotation>
                                        <xs:documentation>regex pattern to match the field name</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string"/>
                                    </xs:simpleType>
                                </xs:attribute>
                                <xs:attribute name="drop_fields">
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string"/>
                                    </xs:simpleType>
                                </xs:attribute>
                                <xs:attribute name="select_fields">
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string"/>
                                    </xs:simpleType>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:all>
        <xs:attribute name="range_date" type="range_date" default="none"/>
        <xs:attribute name="output_range_date" type="range_date" default="none"/>
        <xs:attribute name="id" type="xs:Name" use="required"/>
        <xs:attribute name="description" type="xs:string"/>
        <xs:attribute name="dw_level" type="dw_level"/>
        <xs:attribute name="owner" type="owner:owner"/>
        <xs:attribute name="where" type="xs:string"/>
        <xs:attribute name="is_cache" type="xs:boolean"/>
        <xs:attribute name="computed_columns_ref" type="refs:computed_column_group_id"/>
        <xs:attribute name="where_after_compute" type="xs:string"/>
        <xs:attribute name="distinct_by" type="field_list"/>
        <xs:attribute name="order_by">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:pattern value="(\w+(\s+(asc|desc|ASC|DESC))?,)*\w+(\s+(asc|desc|ASC|DESC))?"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="having" type="xs:string"/>
        <xs:attribute name="drop" type="xs:string"/>
        <xs:attribute name="sort_columns" type="xs:boolean"/>
        <xs:attribute name="is_rollup" type="xs:boolean"/>
        <xs:attribute name="pivot_column" type="xs:string"/>
        <xs:attribute name="group_by" type="xs:string"/>
        <xs:attribute name="measures_ref">
            <xs:simpleType>
                <xs:list itemType="refs:computed_column_group_id"/>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="action_path" abstract="true">
        <xs:complexContent>
            <xs:extension base="action">
                <xs:attribute name="path" type="refs:valid_path"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="source_path">
        <xs:complexContent>
            <xs:extension base="action">
                <xs:attribute name="path" type="refs:valid_path" use="required"/>
                <xs:anyAttribute/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="transform_one_ref" abstract="true">
        <xs:complexContent>
            <xs:extension base="action_path">
                <xs:attribute name="transform_ref" type="transform_source_id" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="transform_multi_refs">
        <xs:complexContent>
            <xs:extension base="action_path">
                <xs:sequence>
                    <xs:element name="transform_refs">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="transform_ref" type="transform_ref" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="sink" abstract="true">
        <xs:complexContent>
            <xs:extension base="action">
                <xs:all>
                    <xs:element name="parents" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="sink_ref">
                                    <xs:complexType>
                                        <xs:attribute name="id" type="refs:sink_id" use="required"/>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:all>
                <xs:attribute name="range_date_sink" type="range_date" default="day_one"/>
                <xs:attribute name="parent" type="refs:sink_id"/>
                <xs:attribute name="sink_t_add" type="xs:int"/>
                <xs:attribute name="sink_cycle" type="sink_cycle"/>
                <xs:attribute name="sink_cycle_pattern" type="xs:string"/>
                <xs:attribute name="transform_ref" type="transform_source_id" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="sink_path">
        <xs:complexContent>
            <xs:extension base="sink">
                <xs:attribute name="path" type="refs:valid_path" use="required"/>
                <xs:anyAttribute/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="source_elastic_search">
        <xs:complexContent>
            <xs:extension base="action">
                <xs:attribute name="es_nodes" type="xs:string" use="required"/>
                <xs:attribute name="es_port" type="xs:int" use="required"/>
                <xs:attribute name="es_table" type="xs:string" use="required"/>
                <xs:attribute name="es_nodes_wan_only" type="xs:boolean"/>
                <xs:attribute name="es_index_auto_create" type="xs:boolean"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="source_hbase">
        <xs:complexContent>
            <xs:extension base="action">
                <xs:sequence>
                    <xs:element name="family">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="column" type="value_node" minOccurs="0"/>
                            </xs:sequence>
                            <xs:attribute name="name" type="xs:string" use="required"/>
                            <xs:attribute name="columns" type="xs:string"/>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="zookeeper_quorum" type="xs:string" use="required"/>
                <xs:attribute name="zookeeper_znode_parent" type="xs:string" use="required"/>
                <xs:attribute name="table_name" type="xs:Name" use="required"/>
                <xs:attribute name="key_start" type="xs:string"/>
                <xs:attribute name="key_end" type="xs:string"/>

            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="source_model">
        <xs:attribute name="model_type" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="k-means"/>
                    <xs:enumeration value="linear-regression"/>
                    <xs:enumeration value="decision-tree"/>
                    <xs:enumeration value="neural-network"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="source_xml">
        <xs:complexContent>
            <xs:extension base="source_path">
                <xs:attribute name="element_path" use="required">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:pattern value="(\w+/)*\w+"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="source_jdbc">
        <xs:complexContent>
            <xs:extension base="action">
